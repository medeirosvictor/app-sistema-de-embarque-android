<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.unifor.sistemadeembarque">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.SistemaDeEmbarque">
        <activity
            android:name="com.unifor.sistemadeembarque.RegisterActivity"
            android:exported="false" />
        <activity
            android:name="com.unifor.sistemadeembarque.LoginActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.unifor.sistemadeembarque.LeituraDeCartao"
            android:exported="false" />
        <activity
            android:name="com.unifor.sistemadeembarque.RelatorioDoDia"
            android:exported="false" />
        <activity
            android:name="com.unifor.sistemadeembarque.MapaDaRota"
            android:exported="false" />
        <activity
            android:name="com.unifor.sistemadeembarque.ListaDePassageiros"
            android:exported="false" />
        <activity
            android:name="com.unifor.sistemadeembarque.OnibusActivity"
            android:exported="false" />
        <activity
            android:name="com.unifor.sistemadeembarque.PassengerActivity"
            android:exported="false" />
        <activity
            android:name="com.unifor.sistemadeembarque.CashierActivity"
            android:exported="false">
        </activity>
    </application>

</manifest>